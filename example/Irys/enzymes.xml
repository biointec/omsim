<?xml version="1.0" encoding="UTF-8"?>
<!--Root containing all enzymes-->
<enzymes>
        <!--properties of the enzyme-->
        <enzyme>
                <!--id, used to specify when this enzyme should be used-->
                <id>BspQI</id>
                <!--enzyme pattern-->
                <pattern>GCTCTTC</pattern>
                <!--number of false positive per 100kbp-->
                <fp>1.0</fp>
                <!--rate of false negatives-->
                <fn>0.15</fn>
        </enzyme>
        <!--Another enzyme-->
        <enzyme>
                <id>BssSI</id>
                <pattern>CACGAG</pattern>
                <fp>1.5</fp>
                <fn>0.20</fn>
        </enzyme>
</enzymes>

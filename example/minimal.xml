<?xml version="1.0" encoding="UTF-8"?>
<!--Root containing all simulations to run-->
<simulation>
        <!--Minimal example, specifying 1 non-circular input file and 1 enzyme-->
        <input>
                <name>Genome Minimal</name>
                <files>
                        <file>genome.fasta</file>
                </files>
                <enzymes>
                        <file>enzymes.xml</file>
                        <enzyme>
                                <id>BspQI</id>
                                <label>label_0</label>
                        </enzyme>
                </enzymes>
        </input>
</simulation>
